{
	"name": "@vangware/utils",
	"description": "⚒️ Vangware curried functional utils",
	"version": "7.0.1",
	"author": {
		"email": "hello@vangware.com",
		"name": "Vangware",
		"url": "https://vangware.com"
	},
	"bugs": "https://github.com/vangware/utils/issues",
	"devDependencies": {
		"@arkweid/lefthook": "^0.7.6",
		"@types/eslint": "7.28.0",
		"@types/node": "16.6.0",
		"@types/prettier": "^2.3.2",
		"@typescript-eslint/eslint-plugin": "4.29.1",
		"@typescript-eslint/parser": "4.29.1",
		"@vangware/base": "5.0.4",
		"@vangware/test": "^7.1.1",
		"c8": "^7.8.0",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-import-resolver-node": "^0.3.5",
		"eslint-plugin-ban": "^1.5.2",
		"eslint-plugin-functional": "^3.5.0",
		"eslint-plugin-import": "2.24.0",
		"eslint-plugin-no-null": "^1.0.2",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"npm-run-all": "^4.1.5",
		"prettier": "2.3.2",
		"rimraf": "^3.0.2",
		"stylelint": "^13.13.1",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-order": "^4.1.0",
		"stylelint-prettier": "^1.2.0",
		"ts-node": "10.2.0",
		"typedoc": "^0.21.5",
		"typescript": "4.3.5"
	},
	"exports": {
		"import": "./dist/import/index.js",
		"require": "./dist/require/index.js"
	},
	"files": [
		"dist"
	],
	"homepage": "https://utils.vangware.com",
	"husky": {
		"hooks": {
			"pre-commit": "run-s clean pre-compile lint test document"
		}
	},
	"keywords": [
		"array",
		"curried",
		"deno",
		"esm",
		"node",
		"object",
		"pure",
		"string",
		"types",
		"typescript",
		"utils",
		"vangware"
	],
	"license": "MIT",
	"repository": "vangware/utils",
	"scripts": {
		"clean": "rimraf ./dist",
		"compile": "run-s compile:*",
		"compile:import": "tsc --project tsconfig.import.json",
		"compile:require": "tsc --project tsconfig.require.json",
		"document": "typedoc --disableOutputCheck --gitRevision main --theme minimal ./src/index.ts",
		"lint": "eslint {src,tests}/**/*.ts",
		"lint:fix": "eslint {src,tests}/**/*.ts --fix",
		"pre-compile": "run-s pre-compile:*",
		"pre-compile:import": "tsc --noEmit --project tsconfig.import.json",
		"pre-compile:require": "tsc --noEmit --project tsconfig.require.json",
		"prepublishOnly": "run-s compile prettify",
		"prettify": "prettier --write --loglevel warn './dist/**/*.{js,ts}'",
		"test": "c8 --reporter=html --reporter=text-summary --reporter=lcov test"
	},
	"types": "./dist/import/index.d.ts"
}
